{
    "name": "pantheon-upstreams/drupal-composer-managed",
    "description": "Install Drupal 9 with Composer on Pantheon.",
    "type": "project",
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "path",
            "url": "upstream-configuration"
        }
    ],
    "require": {
        "pantheon-upstreams/upstream-configuration": "dev-main",
        "composer/installers": "^1.9",
        "drupal/core-composer-scaffold": "^10",
        "drupal/core-recommended": "^10",
        "pantheon-systems/drupal-integrations": "^10",
        "cweagans/composer-patches": "^1.7",
        "drush/drush": "^12.0",
        "drupal/address": "^1.12",
        "drupal/address_static_map": "^1.1",
        "drupal/admin_toolbar": "^3.4",
        "drupal/better_exposed_filters": "^6.0",
        "drupal/dynamic_entity_reference": "^3.0",
        "drupal/easy_breadcrumb": "^2.0",
        "drupal/emulsify_twig": "^4.0",
        "drupal/entity_reference_revisions": "^1.10",
        "drupal/field_group": "^3.4",
        "drupal/focal_point": "^2.0",
        "drupal/honeypot": "^2.1",
        "drupal/key": "^1.17",
        "drupal/key_value_field": "^1.3",
        "drupal/paragraphs": "^1.15",
        "drupal/paragraph_blocks": "^3.1",
        "drupal/paragraphs_edit": "^2.0",
        "drupal/pathauto": "^1.11",
        "drupal/salesforce": "^5.0",
        "drupal/smart_date": "^4.0",
        "drupal/token": "^1.11",
        "drupal/twig_field_value": "^2.0",
        "drupal/twig_tweak": "^3.2",
        "drupal/typed_data": "^1.0@beta",
        "drupal/webform": "^6.2@beta",
        "drupal/components": "^3.0@beta",
        "drupal/block_field": "^1.0@RC",
        "firebase/php-jwt": "^6.5",
        "drupal/ckeditor_link_styles": "^1.0",
        "drupal/csv_importer": "^1.16",
        "wikimedia/composer-merge-plugin": "^2.1",
        "drupal/dropzonejs": "^2.8",
        "enyo/dropzone": "^5.7",
        "drupal/media_bulk_upload": "^3.0",
        "drupal/tamper": "^1.0@alpha",
        "drupal/feeds": "^3.0@beta",
        "drupal/feeds_tamper": "^2.0@beta",
        "drupal/xmlsitemap": "^1.5",
        "drupal/search_api": "^1.29",
        "drupal/sitewide_alert": "^2.1",
        "drupal/animated_gif": "^2.0",
        "drupal/metatag_import_export_csv": "^1.2",
        "drupal/link_field_autocomplete_filter": "^2.0",
        "drupal/google_analytics": "^4.0",
        "drupal/moderation_dashboard": "^2.1",
        "drupal/linkit": "^6.1",
        "drupal/editor_advanced_link": "^2.2",
        "drupal/simplify_menu": "^3.1",
        "drupal/smtp": "^1.2",
        "drupal/redirect": "^1.9",
        "drupal/gin": "^3.0@RC",
        "drupal/taxonomy_manager": "^2.0",
        "drupal/media_video_micromodal": "^1.1"
    },
    "require-dev": {
        "drupal/core-dev": "^10"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "./web"
            },
            "allowed-packages": ["pantheon-systems/drupal-integrations"],
            "file-mapping": {
                "[project-root]/.editorconfig": false,
                "[project-root]/pantheon.upstream.yml": false,
                "[project-root]/.gitattributes": false
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "web/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/profiles/custom/{$name}": ["type:drupal-custom-profile"],
            "web/themes/custom/{$name}": ["type:drupal-custom-theme"],
            "web/private/scripts/quicksilver/{$name}/": ["type:quicksilver-script"]
        },
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "enable-patching": true,
        "merge-plugin": {
            "include": ["web/modules/contrib/dropzonejs/composer.libraries.json"]
        }
    },
    "autoload": {
        "classmap": ["upstream-configuration/scripts/ComposerScripts.php"]
    },
    "scripts": {
        "pre-update-cmd": ["DrupalComposerManaged\\ComposerScripts::preUpdate"],
        "post-update-cmd": ["DrupalComposerManaged\\ComposerScripts::postUpdate"]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": false,
        "platform": {
            "php": "8.1.0"
        },
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true,
            "wikimedia/composer-merge-plugin": true
        }
    }
}
